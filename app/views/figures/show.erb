<table>
  <th>Figure Name</th>
  <tr>
      <td><a href="/figures/<%= @figure.id %>"><%= @figure.name %></a>
      <ul>
      Landmarks:
        <%@figure.landmarks.each do |landmark|%>
        <li><%=landmark.name%> <%=landmark.year_completed%></li>
        <%end%>
      </ul>
      <ul>
      Titles:
        <%@figure.titles.each do |title|%>
        <li><%=title.name%></li>
        <%end%>
      </ul>
  </tr>
      </td>
</table>
